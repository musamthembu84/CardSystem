@model LostCard.Models.mvcCards
@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>



<div class="form-group">

    @using (Html.BeginForm())
    {
        <div class="form-group">

            @Html.LabelFor(model => model.SNumber)
            @Html.EditorFor(model => model.SNumber)
            @Html.ValidationMessageFor(model => model.SNumber)

        </div>


       
        <div class="form-group">
            <input type="submit" value="Search" />
        </div>
    }


</div>

@section scripts{

    @Scripts.Render("~/bundles/jqueryval");
}

